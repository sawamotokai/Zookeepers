<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Zookeeper</title>
  <link rel="stylesheet" href="/styles.css">
</head>

  <body>
    <div class="page">
      <div class="header">
          <h1 class="title">Guests</h1>
          </p>
      </div>
      
      <h2>Guests who have entered the zoo</h2>
      <ul>
        <% for (var i =0; i < guests.length;  i++ ) { %>
          <li/>  <%= guests[i].Number %> <%= guests[i].Age %>
          <% } %>
      </ul>
      <h2>New Guest Entry</h2>
      <form action="/guest/new" method="POST" class="form">
        <label for="guestName">Name</label>
        <input id="guestName" type="text" name="name" class="input" placeholder="Bob" required>
        <button type="submit" class="btn">Create!</button>
      </form>
      <button onclick="func1()">Do something!</button>
    </div>
  </body>
  <script>
    function func1(){
      console.log("I am in the click handler 1!"); 
    }
  
    function func2(){
      console.log("I am in the click handler 2!"); 
    }
  </script>

</html>
